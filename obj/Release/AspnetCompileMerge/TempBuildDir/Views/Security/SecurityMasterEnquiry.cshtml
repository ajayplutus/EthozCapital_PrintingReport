@{
    ViewBag.Title = "Security Master Enquiry";
}
@if (ViewData["Message"] != null)
{
    <h2>Invalid Page</h2>
    @ViewData["Message"]
}
else
{
   <div class="view-header-title">
        <h2>Security Master Enquiry</h2>
    </div>
   <body>
       <script src="~/CustomScript/Security/security-master-enquiry.module.js" asp-append-version="true"></script>
       <div class="panel panel-default panel-with-tabs" data-sortable-id="ui-unlimited-tabs-2" id="divSecurityInq" style="overflow:auto">
           <link href="~/CustomCss/Security/security-master-enquiry.css" rel="stylesheet" />
           <link href="~/Content/common.css" rel="stylesheet" />
           @{Html.RenderAction("SecurityMasterEnquirySearchView", new { viewType = "View" });}

           <div id="divSearchResult" class="margin-15">
               @{Html.RenderAction("SecurityMasterEnquiryListView", new { viewType = "View" });}
           </div>
       </div>
       <a href="javascript:;" class="btn btn-icon btn-circle btn-primary btn-scroll-to-top fade" data-click="scroll-top"><i class="fa fa-angle-up"></i></a>
   </body>
                   }